<table id="mytable" class="table table-bordred table-striped" >
    <thead>

    {#<th><input type="checkbox" id="checkall" /></th>#}
    <th>Nom</th>
    <th>Prenom</th>
    <th>Email</th>
    {#<th>Spécialité</th>#}
    {#<th>Address</th>#}

    <th>Modifier</th>
    <th>Supprimer</th>
    <th>Validation</th>
    <th>Bloquer</th>
    </thead>
{% for i in all  %}

    <tbody>

    <tr>
    {#<td><input type="checkbox" class="checkthis" /></td>#}
    <td>{{ i.nom }}</td>
    <td>{{ i.prenom }}</td>
    <td>{{ i.email }}</td>
    <td>{{ i.specialite }}</td>
    <td>{{ i.adresse }}</td>
    <td><p data-placement="top" data-toggle="tooltip" title="Edit"><button  class="btn btn-primary btn-xs" data-title="Edit" data-toggle="modal" data-target="#edit " ><span class="glyphicon glyphicon-pencil"></span></button></p></td>
    {#<td><p data-placement="top" data-toggle="tooltip" title="Delete"><button  data-id="{{ i.id }}"  class="btn btn-danger btn-xs" id="a" data-title="Delete" data-toggle="modal" data-target="#delete"><span class="glyphicon glyphicon-trash"></span></button></p></td>#}


    </tr>





{% endfor %}
    </tbody>
</table>